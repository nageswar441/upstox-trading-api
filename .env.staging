# ============================================
# STAGING ENVIRONMENT
# ============================================

# Upstox API Credentials (Use separate account for staging)
UPSTOX_API_KEY=your_staging_api_key
UPSTOX_API_SECRET=your_staging_api_secret
UPSTOX_API_TOKEN=your_staging_access_token

# API URLs
UPSTOX_BASE_URL=https://api.upstox.com/v2
UPSTOX_WEBSOCKET_URL=wss://api.upstox.com/v2/feed/market-data-feed

# Security - Use strong keys
INTERNAL_API_KEY=staging_strong_api_key_replace_with_real
WEBHOOK_SECRET=staging_webhook_secret_replace_with_real

# CORS - Restrict to staging domain
ALLOWED_ORIGINS=https://staging.yourdomain.com,https://staging-api.yourdomain.com

# Application
APP_ENV=staging
APP_PORT=8000
APP_HOST=0.0.0.0
LOG_LEVEL=INFO

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_PER_MINUTE=100

# Debug Mode
DEBUG=false
RELOAD=false
