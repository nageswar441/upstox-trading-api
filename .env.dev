# ============================================
# DEVELOPMENT ENVIRONMENT
# ============================================

# Upstox API Credentials
UPSTOX_API_KEY=your_dev_api_key
UPSTOX_API_SECRET=your_dev_api_secret
UPSTOX_API_TOKEN=your_dev_access_token

# API URLs (Upstox doesn't have separate dev API, use production)
UPSTOX_BASE_URL=https://api.upstox.com/v2
UPSTOX_WEBSOCKET_URL=wss://api.upstox.com/v2/feed/market-data-feed

# Security
INTERNAL_API_KEY=dev_api_key_12345_use_real_key_in_production
WEBHOOK_SECRET=dev_webhook_secret_12345

# CORS - Allow all localhost origins for development
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001,http://localhost:8000,http://127.0.0.1:3000

# Application
APP_ENV=development
APP_PORT=8000
APP_HOST=0.0.0.0
LOG_LEVEL=DEBUG

# Rate Limiting - More relaxed for development
RATE_LIMIT_ENABLED=true
RATE_LIMIT_PER_MINUTE=120

# Debug Mode
DEBUG=true
RELOAD=true
